push 0
store 0 //INPUT_CLOCK
push 1
store 1 //INPUT_DISTANCE
push 2
store 2 //INPUT_COLOR_LEFT
push 3
store 3 //INPUT_COLOR_RIGHT
read 0 0 //clock = read INPUT_CLOCK
read 1 1 //distance = read INPUT_DISTANCE
read 2 2 //color_l = read INPUT_COLOR_LEFT
read 3 3 //color_r = read INPUT_COLOR_RIGHT
halt
